{
    "name": "nvidia-nano-demo-module",
    "version": "1.0.30",
    "description": "NVIDIA IoT Central module",
    "main": "index.js",
    "scripts": {
      "test": "node ./node_modules/jest/bin/jest.js",
      "tslint": "node ./node_modules/tslint/bin/tslint -p ./tsconfig.json",
      "build": "node ./node_modules/typescript/bin/tsc -p .",
      "dockerbuild": "node ./scripts/dockerBuild.js --docker-build",
      "preversion": "npm test",
      "version": "node ./scripts/dockerBuild.js --docker-build",
      "postversion": "node ./scripts/dockerBuild.js --docker-push",
      "postinstall": "node ./scripts/setupDevEnvironment.js"
    },
    "author": "sseiber",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git@github.com:sseiber/nvidia-nano-demo-module.git"
    },
    "dependencies": {
      "@hapi/boom": "^7.4.9",
      "@hapi/good": "^8.2.0",
      "@hapi/good-console": "^8.1.0",
      "@hapi/good-squeeze": "^5.2.0",
      "@hapi/hapi": "^18.4.0",
      "@hapi/inert": "^5.2.0",
      "@hapi/nes": "^11.1.0",
      "azure-iot-device": "^1.11.2",
      "azure-iot-device-mqtt": "^1.10.3",
      "fs-extra": "^7.0.1",
      "lodash.defaults": "^4.2.0",
      "lodash.get": "^4.4.2",
      "lodash.padstart": "^4.6.1",
      "lodash.random": "^3.2.0",
      "lodash.set": "^4.3.2",
      "nconf": "^0.10.0",
      "pjson": "^1.0.9",
      "query-string": "^6.4.2",
      "request": "^2.88.0",
      "rimraf": "^2.6.3",
      "spryly": "^1.0.19",
      "uuid": "^3.3.2"
    },
    "devDependencies": {
      "@types/azure": "^0.9.20",
      "@types/jest": "^24.0.11",
      "@types/nconf": "0.10.0",
      "@types/node": "^11.12.2",
      "@types/request": "^2.48.1",
      "jest": "^24.5.0",
      "ts-jest": "^24.0.1",
      "tslint": "^5.19.0",
      "typescript": "^3.6.2"
    },
    "jest": {
      "rootDir": "./src/",
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
      "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "json"
      ],
      "transform": {
        ".tsx?": "ts-jest"
      },
      "moduleNameMapper": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js"
      }
    }
  }